{
  "name": "191413ai-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "NODE_OPTIONS='--max-old-space-size=4096' eslint src",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build && echo 'storybook.191413.ai' > storybook-static/CNAME && touch storybook-static/.nojekyll",
    "predeploy": "yarn build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^15.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.8.1",
    "@emotion/react": "11.14.0",
    "@eslint/js": "9.31.0",
    "@storybook/addon-a11y": "9.0.18",
    "@storybook/addon-docs": "8.0.4",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-interactions": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/cli": "9.0.18",
    "@storybook/nextjs": "7.6.20",
    "@storybook/react": "7.6.20",
    "@storybook/react-dom-shim": "9.0.18",
    "@storybook/react-vite": "8.0.4",
    "@storybook/testing-library": "0.2.0",
    "@tailwindcss/postcss": "4.1.11",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.44",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@vitejs/plugin-react": "4.7.0",
    "ajv": "8.11.0",
    "ajv-keywords": "5.1.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.1.0",
    "eslint-config-next": "^15.4.4",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.3",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-regexp": "2.9.0",
    "gh-pages": "^6.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "standard-version": "9.5.0",
    "tailwindcss": "4.1.11",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vite": "^5.2.0",
    "vitest": "3.2.4"
  },
  "overrides": {
    "eslint": "9.1.0",
    "rollup": "4.14.0",
    "electron-to-chromium": "1.5.190"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss,yml}": [
      "prettier --write"
    ]
  }
}
