  "eslint.validate": ["typescript", "typescriptreact"],
  "eslint.experimental.useFlatConfig": true,
  "prettier.enable": true,
  "prettier.configPath": ".prettierrc.json",
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
